    "# Importar librerías\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn import linear_model\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Cargar los datos\n",
    "# Asegúrate de que el archivo canada_per_capita_income.csv está en una carpeta \"datasets\" o al mismo nivel\n",
    "df = pd.read_csv(r\"Exercise\\canada_per_capita_income.csv\")\n",
    "\n",
    "# Mostrar las primeras filas para verificar\n",
    "print(df.head())\n",
    "\n",
    "# Renombrar columnas si es necesario (dependerá del archivo original)\n",
    "# Ejemplo: Year, Per capita income (US$)\n",
    "df.columns = ['year', 'income']\n",
    "\n",
    "# Crear el modelo de regresión lineal\n",
    "reg = linear_model.LinearRegression()\n",
    "\n",
    "# Entrenar el modelo\n",
    "reg.fit(df[['year']], df.income)\n",
    "\n",
    "# Visualizar la línea de regresión\n",
    "plt.scatter(df.year, df.income, color='blue', label='Datos reales')\n",
    "plt.plot(df.year, reg.predict(df[['year']]), color='red', label='Regresión lineal')\n",
    "plt.xlabel('Año')\n",
    "plt.ylabel('Ingreso per cápita (US$)')\n",
    "plt.legend()\n",
    "plt.title('Ingreso per cápita en Canadá')\n",
    "plt.show()\n",
    "\n",
    "# Predecir el ingreso per cápita para el año 2020\n",
    "prediccion_2020 = reg.predict([[2020]])\n",
    "print(f\"💰 Ingreso per cápita estimado en Canadá para 2020: ${prediccion_2020[0]:.2f} USD\")\n"
   ]
